# Test Environment Variables
# Copy this file to .env.test and replace the placeholder values

# GitHub Personal Access Token (required for integration tests)
# Get yours at: https://github.com/settings/tokens
# Required scope: public_repo (read access to public repositories)
GH_PAT=your_github_token_here

# App URL for testing (default: http://localhost:3000)
# Change this if your dev server runs on a different port
NEXT_PUBLIC_APP_URL=http://localhost:3000

# GitHub Organization to test against
GITHUB_ORG=deepgram-starters